{% extends 'baseelements/themebase.html.twig' %}

{% block pagebody %}
    <h1>Invoiceproduct</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ invoiceProduct.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ invoiceProduct.quantity }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('invoiceproduct_index',{'invoiceid':app.request.get('invoiceid')}) }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('invoiceproduct_edit', { 'id': invoiceProduct.id,'invoiceid':app.request.get('invoiceid') }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
